    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Default",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}"
        },
        {
            "name": "Launch Custom ENV",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "env": {
                "ECHOIP_HOST": "127.0.0.1",
                "ECHOIP_PORT": "8081"
            },
        }
    ]
}